{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"ricbra/php-discogs-api": "~1.0.0",
    "codesleeve/asset-pipeline": "dev-master",
    "schnubor/phpclasses-sasl": "dev-master",
		"schnubor/phpclasses-httpclient": "dev-master",
		"schnubor/phpclasses-oauth-api": "dev-master"
	},
	"require-dev": {
		"way/generators": "2.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable",
	"repositories":
  [
    {
      "type": "vcs",
      "url": "https://github.com/schnubor/phpclasses-oauth-api.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/schnubor/phpclasses-httpclient.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/schnubor/phpclasses-sasl.git"
    }
  ]
}
